package visitor;
import syntaxtree.*;
import java.util.*;

class Obj {
	String type;
	int index;
	String cname;
	int offset;
}




class MethodData {
	String mthdname;
	String returnType;
	int index;
	String cname;
	
	int mthdoffset;
	
	LinkedHashMap<String, String> params;
	LinkedHashMap<String, String> vars;
}




class class_info {
	LinkedHashMap<String, MethodData> methods = new LinkedHashMap<String, MethodData>();
	int count_method;
	
	LinkedHashMap<String, Obj> vars = new LinkedHashMap<String, Obj>();
	int count_var;
	
	
	int fieldoffset;
	int methodoffset;
	
	
	
	public void add_var(String name, String type, String clssnm) {
		Obj o = new Obj();
		o.index = count_var;
		o.cname = clssnm;
		vars.put(clssnm+"_"+name, o);
		count_var++;
	}
	
	public void add_method(String name, String clss, String type) {	
//		Obj o = new Obj();
		MethodData mthd = new MethodData();
		mthd.index = count_method;
//		o.index = count_method;
		mthd.cname = clss;
		mthd.returnType = type;
		mthd.params = new LinkedHashMap<String, String>();
		mthd.vars = new LinkedHashMap<String, String>();
		methods.put(name, mthd);
		count_method++;
	}

	
	public void print(boolean debug) {
		if (debug) {
		System.out.println("Methods:");
//		System.out.println(methods);
		for (String mthdname: methods.keySet() ) {
//			String key = mthd;
			MethodData mthd = methods.get(mthdname);
			System.out.println(mthdname + ": " + "\n \t Class: " + mthd.cname + "\n \t type:  " + mthd.returnType);
		}
		
		System.out.println("Vars:");
//		System.out.println(vars);
		for (String varname: vars.keySet() ) {
//			String key = mthd;
			Obj var = vars.get(varname);
			System.out.println(varname + ": " + "\n \t Class: " + var.cname + "\n \t type:  " + var.type);
		}
		}
	}
}


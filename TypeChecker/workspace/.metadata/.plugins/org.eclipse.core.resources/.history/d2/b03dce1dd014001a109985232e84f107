package visitor;
import syntaxtree.*;
import java.util.*;

/**
 * Provides default methods which visit each node in the tree in depth-first
 * order.  Your visitors may extend this class.
 */
public class LLVM_generator<R,A> extends GJDepthFirst<R,A> {
	
	boolean debug = true;
	
	public LinkedHashMap<String, class_info> classhm = new LinkedHashMap<String, class_info>();
	
	int temp_count = 0;
	int label_count = 0;
	String curr_class = "";
	String curr_mthd = "";
	
	public String main_class = "";
	
	
	String params_allocate = "";
	
	
	
	String newTemp() {
		temp_count++;
		return Integer.toString(temp_count);
	}
	
	
	String newLabel() {
		label_count++;
		return Integer.toString(label_count);
	}
	
	
	void print(String s) {
		if (debug)
			System.out.println(s);
	}
	
	
	void llvm_methods() {
		System.out.println("\n" +
                "declare i8* @calloc(i32, i32)\n" +
                "declare i32 @printf(i8*, ...)\n" +
                "declare void @exit(i32)\n" +
                "\n" +
                "@_cint = constant [4 x i8] c\"%d\\0a\\00\"\n" +

                "define void @print_int(i32 %i) {\n" +
                "    %_str = bitcast [4 x i8]* @_cint to i8*\n" +
                "    call i32 (i8*, ...) @printf(i8* %_str, i32 %i)\n" +
                "    ret void\n" +
                "}\n");
	}
	
	
	void printVTables() {
		// Print main class vtable
		System.out.println("@." + main_class + "_vtable = global [0 x i8*] []");
		
		// Generate other class vtables
		for (String curr_class : classhm.keySet()) {
			class_info classdata = classhm.get(curr_class);
			
			System.out.println("@." + curr_class + "_vtable = global [" +
								Integer.toString(classdata.count_method) + " x i8*] [");
			
			String vtable = "";
			boolean comma = false;
			
			for (String curr_mthd : classdata.methods.keySet()) {
				MethodData mthddata = classdata.methods.get(curr_mthd);
				
				String retType = mthddata.returnType;
				
				if (comma)
					vtable += ", ";
				
				vtable += "i8* bitcast (";
				
				if (retType.equals("int")) 
					vtable += "i32 (i8*";
				else if (retType.equals("boolean"))
					vtable += "i1 (i8*";
				else if (retType.equals("int[]"))
					vtable += "i32* (i8*";
				else
					vtable += "i8* (i8*";
				
				
				// Setup arguments
				for (String arg : mthddata.params.keySet()) {
					String type = mthddata.params.get(arg);
					
					if (type.equals("int"))
						vtable += ", i32";
					else if (type.equals("boolean"))
						vtable += ", i1";
					else if (type.equals("int[]"))
						vtable += ", i32*";
					else
						vtable += ", i8*";
				}
				
				vtable += ")* @" + curr_class + "." + curr_mthd + " to i8*)";
				
				System.out.print(vtable);
				comma = true;
				vtable = "";
			}
			
			System.out.print("]");
		}
	}
	
	
	String giveLLVMtype(String s) {
//		print(s);
		if (s.equals("int"))
			return "i32";
		else if (s.equals("boolean"))
			return "i1";
		else if (s.equals("int[]"))
			return "i32*";
		else
			return "i8*";
	}
	

	
	String lookup_identifier(String id) {
		class_info classdata = classhm.get(curr_class);
		MethodData curr_mthd_data = classdata.methods.get(curr_mthd);
		
		if (curr_mthd_data.params.containsKey(id))
			return "parameter";
		else if (curr_mthd_data.vars.containsKey(id))
			return "variable";
		else if (classdata.vars.containsKey(id))
			return "field";
		else {
			System.out.println("Fuckup");
			return null;
		}
	}
	
	
	

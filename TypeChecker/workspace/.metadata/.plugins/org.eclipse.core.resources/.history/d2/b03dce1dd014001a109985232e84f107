      String type = (String) n.f0.accept(this, argu);
//      n.f1.accept(this, argu);
      String id = n.f1.f0.tokenImage;
      
      System.out.println("%" + id + " = alloca " + giveLLVMtype(type));
      
//      if (type.equals("int"))
//    	  System.out.print("i32");
//      else if  (type.equals("boolean"))
//    	  System.out.print("i1");
